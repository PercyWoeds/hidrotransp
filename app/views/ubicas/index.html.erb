<p id="notice"><%= notice %></p>

<h1>Listing Ubicas</h1>

<table>
  <thead>
    <tr>
      <th>Descrip</th>
      
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ubicas.each do |ubica| %>
      <tr>
        
        <td><%= ubica.descrip %></td>
        
        <td><%= ubica.comments %></td>
        <td><%= link_to 'Ver', ubica %></td>
        <td><%= link_to 'Editar', edit_ubica_path(ubica) %></td>
        <td><%= link_to 'Eliminar', ubica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ubica', new_ubica_path %>
